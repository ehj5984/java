package com.kopo.project0521;

public class Student {
	int idx;
	String name;
	int middleScore;
	int finalScore;
	String created;

	Student() {

	}

	Student(String name, int middleScore, int finalScore, String created) {
		this.name = name;
		this.middleScore = middleScore;
		this.finalScore = finalScore;
		this.created = created;
	}
}